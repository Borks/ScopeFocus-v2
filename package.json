{
	"name": "scopefocus",
	"displayName": "ScopeFocus",
	"description": "Focus only on a part of your code",
	"version": "0.0.1",
	"publisher": "BorkaCodes",
	"icon": "logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/Borks/ScopeFocus-v2"
	},
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.focus"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
		{
			"command": "extension.focus",
			"title": "ScopeFocus: Activate"
		},
		{
			"command": "extension.defocus",
			"title": "ScopeFocus: Deactivate"
		}
	]},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
